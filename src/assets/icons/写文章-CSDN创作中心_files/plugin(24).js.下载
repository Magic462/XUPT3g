(function(){CKEDITOR.plugins.add("switchmarkdown",{icons:"switchmarkdown",icontype:".svg",iconoffset:"center",iconbgsize:"16px",hidpi:!1,init:function(a){a.addCommand("switchToMarkdown",{exec:function(a){a.config.switchToMarkdown&&a.config.switchToMarkdown()}});a.ui.addButton("switchmarkdown",{label:"使用 MD 编辑器",command:"switchToMarkdown",toolbar:"insert,40"})}})})();