CKEDITOR.plugins.add("addtpl",{icons:"addtpl",icontype:".svg",iconoffset:"center",iconbgsize:"16px",hidpi:!1,beforeInit:function(a){},init:function(a){a.addCommand("execAppendTpl",{exec:function(a){a.config.addTpl?a.config.addTpl.execAppendTpl(a):a.getCommand("addtpl").exec()}});a.ui.addButton&&a.ui.addButton("addTplOutside",{icon:"addtpl",label:"模版",command:"execAppendTpl",toolbar:"insert,18"});a.ui.addButton("addtpl",{label:"模版",command:"addtpl",toolbar:"insert,18"});a.addCommand("addtpl",new CKEDITOR.dialogCommand("addtplDialog"));
CKEDITOR.dialog.add("addtplDialog",this.path+"dialogs/addtpl.js");CKEDITOR.document.appendStyleSheet(this.path+"styles/tpl.css")},onLoad:function(a){},afterInit:function(a){}});