CKEDITOR.plugins.add("format",{requires:"richcombo",lang:"zh-cn",icons:"format",icontype:".svg",iconoffset:"center",iconbgsize:"16px",hidpi:!1,init:function(a){if(!a.blockless){for(var g=a.config,f=a.lang.format,m=g.format_tags.split(";"),e={},h=0,n=[],k=0;k<m.length;k++){var c=m[k],d=new CKEDITOR.style(g["format_"+c]);if(!a.filter.customConfig||a.filter.check(d))h++,e[c]=d,e[c]._.enterMode=a.config.enterMode,n.push(d)}c="p";d=new CKEDITOR.style(g["format_"+c]);if(!a.filter.customConfig||a.filter.check(d))h++,
e[c]=d,e[c]._.enterMode=a.config.enterMode,n.push(d);0!==h&&a.ui.addRichCombo("Format",{icon:"format",label:f.label,title:f.panelTitle,panel:{css:[CKEDITOR.skin.getPath("editor")].concat(g.contentsCss),multiSelect:!1,attributes:{"aria-label":f.panelTitle}},init:function(){this.startGroup(f.panelTitle);for(var b in e){var l=f["tag_"+b];if(-1<CKEDITOR.config.format_tags.indexOf(b))this.add(b,'\x3cspan class\x3d"cke_combo_icon cke_combo_icon__format cke_button_icon cke_combo_icon__format_'+b+'"\x3e'+
l+"\x3c/span\x3e",b);else{var c=CKEDITOR.skin.getIconStyle(b,"rtl"==a.lang.dir,void 0,void 0);this.add(b,'\x3cspan class\x3d"cke_combo_icon cke_combo_icon__format cke_button_icon cke_combo_icon__panellist" style\x3d"'+c+'"\x3e'+l+"\x3c/span\x3e",l)}}},onClick:function(b){a.focus();a.fire("saveSnapshot");b=e[b];var c=a.elementPath();b.checkActive(c,a)?a.applyStyle(e.p):a.applyStyle(b);setTimeout(function(){a.fire("saveSnapshot")},0)},onRender:function(){a.on("selectionChange",function(b){var c=this.getValue();
b=b.data.path;this.refresh();for(var d in e)if(e[d].checkActive(b,a)){d!=c&&"p"!=d&&this.setValue(d,a.lang.format["tag_"+d]);return}this.setValue("")},this)},onOpen:function(){this.showAll()},refresh:function(){var b=a.elementPath();if(b){if(b.isContextFor("p"))for(var c in e)if(a.activeFilter.check(e[c]))return;this.setState(CKEDITOR.TRISTATE_DISABLED)}}})}}});CKEDITOR.config.format_tags="p;h1;h2;h3;h4;h5;h6";CKEDITOR.config.format_p={element:"p"};CKEDITOR.config.format_div={element:"div"};
CKEDITOR.config.format_pre={element:"pre"};CKEDITOR.config.format_address={element:"address"};CKEDITOR.config.format_h1={element:"h1"};CKEDITOR.config.format_h2={element:"h2"};CKEDITOR.config.format_h3={element:"h3"};CKEDITOR.config.format_h4={element:"h4"};CKEDITOR.config.format_h5={element:"h5"};CKEDITOR.config.format_h6={element:"h6"};